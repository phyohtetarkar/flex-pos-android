<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jsoft.es">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_pos"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_pos"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".EasyShopApplication">
        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.views.unit.UnitActivity"/>
        <activity android:name=".ui.views.category.CategoryActivity" />
        <activity android:name=".ui.views.item.EditItemActivity" />
    </application>

</manifest>